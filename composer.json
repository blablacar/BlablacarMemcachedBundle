{
    "name": "blablacar/memcached-bundle",
    "description": "BlaBlarCar Memcached bundle",
    "keywords": ["symfony2","memcached","bundle"],
    "license": "MIT",
    "authors": [
        {
            "name": "Olivier Dolbeau",
            "homepage": "http://odolbeau.fr/"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "blablacar/memcached-client": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7",
        "phpspec/prophecy": "~1.1",
        "symfony/http-kernel": "@stable",
        "symfony/config": "@stable",
        "symfony/dependency-injection": "@stable"
    },
    "autoload": {
        "psr-0": {
            "Blablacar": "src"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": { "Blablacar\\MemcachedBundle": "" }
    },
    "target-dir": "Blablacar/MemcachedBundle"
}
